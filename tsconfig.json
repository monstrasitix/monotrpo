{
    "compilerOptions": {
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "moduleResolution": "Bundler",
        "outDir": "./dist",
        "jsx": "react-jsxdev",
        "target": "ES2022",
        "strict": true,
        "paths": {
            "@/base/*": ["./apps/react-base/src/*"]
        }
    },
    "include": [
        "./webpack.*.ts",
        "./apps/react-base/src/*",
        "./apps/*"
    ],
    "exclude": []
}